{
  "name": "react-draggable-tab",
  "version": "0.0.1",
  "description": "Draggable chrome like tab react component ",
  "main": "index.js",
  "scripts": {
    "browser": "browser-sync start --files example/* --server example",
    "watch:example": "watchify example/app.js -dv -o example/bundle.js",
    "start:example": "npm run watch:example & npm run browser",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/georgeosddev/react-draggable-tab"
  },
  "keywords": [
    "react",
    "tab",
    "draggable"
  ],
  "author": "Takeharu.Oshida",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georgeosddev/react-draggable-tab/issues"
  },
  "homepage": "https://github.com/georgeosddev/react-draggable-tab",
  "devDependencies": {
    "browserify": "^10.2.1",
    "watchify": "^3.2.1",
    "babelify": "^6.1.2",
    "browser-sync": "^2.7.6",
    "babel-eslint": "^3.1.9",
    "eslint": "^0.21.2",
    "eslint-plugin-react": "^2.3.0"
  },
  "dependencies": {
    "immutable": "^3.7.3",
    "react": "^0.13.3",
    "react-draggable": "^0.8.0",
    "react-style": "^0.5.5",
    "classnames": "^2.1.2"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "compact": false
        }
      ]
    ]
  }
}
